---
layout: article_wrapper
pageable: true
---

<% page_articles.each do |article| %>
    <%= partial '/blog/article', :locals => {:article => article, :digest => true} %>
<% end %>

<%= partial '/blog/pagination', :locals => {
  :paginate => paginate,
  :next_page => next_page,
  :prev_page => prev_page,
  :num_pages => num_pages,
  :page_number => page_number
} %>
